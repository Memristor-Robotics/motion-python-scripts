#!/usr/bin/python3 -i

import os,sys
sys.path.append(os.path.join(os.path.dirname(os.path.realpath(__file__)), os.pardir, 'python'))

from Can import *
Can.init()

print('CAN cmd')

print('use: \n\
	c.actuator(which, value)\n\
	c.servo(which, func, value)\n\
	listServoCmds()\n\
	listServo()\n\
	listActuators()\n\
	c.read()\n\
	c.read(addr)\n\
	c.debug = 1\n\
	')
from Can import *
c = Can()

def t(x): global c; c.send(bytes([x]), 0x8d53)
