#!/usr/bin/python3
import os,sys
sys.path.append(os.path.join(os.path.dirname(os.path.realpath(__file__)), os.pardir, 'python'))
from Engine import *
e=Engine()


if len(sys.argv) == 2:
	if sys.argv[1] == 'list':
		e.conf_list()
	else:
		print( e.conf_get(sys.argv[1]) )

elif len(sys.argv) >= 3:
	if not e.conf_set(sys.argv[1], sys.argv[2]):
		print("no such config")


